CREATE TABLE IF NOT EXISTS timesheet (
    id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    name TEXT,
    client_name TEXT,
    project_name TEXT,
    from_date INTEGER,
    to_date INTEGER,
    break_in_min INTEGER,
    hourly_rate INTEGER,
    status TEXT, `comment` TEXT)